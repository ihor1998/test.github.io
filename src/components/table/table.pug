table.table#table
    thead
        tr
            -arr = ['Time/Day', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday']
            each elem in arr 
                th.table__col.head #{elem}
    tbody
        -arr = ['10:00', '11:00', '12:00', '13:00', '14:00', '15:00', '16:00', '17:00', '18:00']
        each elem in arr
            tr
                td.table__col.time #{elem}
                td.table__col(data-time=elem, data-day='Monday')
                td.table__col(data-time=elem, data-day='Tuesday')
                td.table__col(data-time=elem, data-day='Wednesday')
                td.table__col(data-time=elem, data-day='Thursday')
                td.table__col(data-time=elem, data-day='Friday')