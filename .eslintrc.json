{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-param-reassign": 0,
        "no-use-before-define": [
            "error",
            {
                "functions": false,
                "classes": true
            }
        ]
    }
}
